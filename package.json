        "jest": "^29.7.0",
        "jest-dom": "^4.0.0",
        "jest-environment-jsdom": "^29.7.0",
        "lint-staged": "^15.2.0",
        "prettier": "^3.2.4",
        "sass": "^1.70.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.2.2",
        "vite": "^5.0.8",
        "vite-plugin-environment": "^1.1.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
