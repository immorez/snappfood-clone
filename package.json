        "react": "^18.2.0",
        "react-cool-virtual": "^0.7.0",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-i18next": "^14.0.1",
        "react-icons": "^5.0.1",
        "react-redux": "^9.1.0",
        "react-router-dom": "^6.21.3",
        "react-spinners": "^0.13.8",
        "react-toastify": "^10.0.4",
        "react-window": "^1.8.10"
